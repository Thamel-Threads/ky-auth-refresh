{
  "name": "@thamel-threads/ky-auth-refresh",
  "version": "1.1.0",
  "description": "Minimal automatic authentication token refresh for Ky HTTP client",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "ky",
    "authentication",
    "authorization",
    "refresh-token",
    "access-token",
    "http-client",
    "fetch",
    "typescript",
    "esm"
  ],
  "author": "Thamel Threads",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thamel-threads/ky-auth-refresh.git"
  },
  "bugs": {
    "url": "https://github.com/thamel-threads/ky-auth-refresh/issues"
  },
  "homepage": "https://github.com/thamel-threads/ky-auth-refresh#readme",
  "peerDependencies": {
    "ky": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "tsup": "^8.0.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
